<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.ddsurok.datamodel.Session" table="m_session">
        <property name="AuthToken" type="java.lang.Integer">
            <column name="authtoken" />
        </property>
        <property name="Id" type="java.util.UUID">
            <column name="id" />
        </property>
        <many-to-one name="user" type="ru.ddsurok.datamodel.User" cascade="all" unique="true" not-null="true" column="user" />
        <property name="CreateTime" type="java.util.Date">
            <column name="createtime" />
        </property>
        <property name="LastUpdateTime" type="java.util.Date">
            <column name="lastupdatetime" />
        </property>
        <property name="LiveTime" type="java.lang.Integer">
            <column name="livetime" />
        </property>
    </class>
</hibernate-mapping>
