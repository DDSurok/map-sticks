<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.ddsurok.datamodel.User" table="m_user">
        <id name="Id" type="java.lang.Integer">
            <column name="id" />
            <generator class="sequence">
                <param name="sequence">m_user_id_seq</param>
            </generator>
        </id>
        <property name="NickName" type="java.lang.String">
            <column name="nick" length="100" not-null="true" unique="true" />
        </property>
        <property name="HashPswd" type="java.lang.String">
            <column name="hashpswd" length="64" not-null="true" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="email" length="255" not-null="true" unique="true" />
        </property>
        <property name="Family" type="java.lang.String">
            <column name="family" length="255" />
        </property>
        <property name="Name" type="java.lang.String">
            <column name="name" length="255" />
        </property>
        <property name="IsLocked" type="java.lang.Boolean">
            <column name="islocked" not-null="true" />
        </property>
        <property name="IsBaned" type="java.lang.Boolean">
            <column name="isbaned" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
